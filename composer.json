{
    "name": "kocsismate/di-container-benchmarks",
    "description": "Benchmarks of some popular PHP DI Containers",
    "license": "MIT",
    "homepage": "https://github.com/kocsismate/php-di-benchmarks",
    "keywords": ["Dependency Injection Container benchmarks", "Dependency Injection Container performance", "DI"],
    "authors": [
        {
            "name": "Máté Kocsis",
            "email": "kocsismate@woohoolabs.com"
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "aura/di": "^4.0.0",
        "bitexpert/disco": "^0.10.0",
        "chubbyphp/chubbyphp-container": "^1.1.0",
        "clancats/container": "^1.2.2",
        "doctrine/cache": "^1.6.1",
        "illuminate/container": "^6.8.0",
        "joomla/di": "^1.5.1",
        "jshannon63/cobalt": "^1.2.1",
        "laminas/laminas-servicemanager": "^3.4.0",
        "level-2/dice": "^4.0.1",
        "opulence/ioc": "^1.1.7",
        "php-di/php-di": "^6.0.11",
        "pimple/pimple": "^3.2.3",
        "psr/container": "^1.0.0",
        "rdlowrey/auryn": "^1.4.2",
        "symfony/dependency-injection": "dev-master as 5.1",
        "thecodingmachine/yaco": "^1.0.0",
        "woohoolabs/zen": "3.0.x-dev",
        "yiisoft/yii2": "^2.0.31"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "squizlabs/php_codesniffer": "^3.5.6",
        "woohoolabs/coding-standard": "^2.2.0"
    },
    "autoload": {
        "psr-4": {
            "DiContainerBenchmarks\\": "src/"
        },
        "files": [
            "src/Container/PhpDi/Resource/CompiledSingletonContainer.php"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse --level 8 src",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf"
    },
    "config": {
        "bin-compat": "full",
        "platform": {
            "php": "8.0.0"
        },
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
